<mvc:View
	controllerName="sap.ui.demo.nav.controller.material.Material"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.ui.layout.form"
	busyIndicatorDelay="0">
	<Page
		id="materialPage"
		title="{i18n>MaterialDetailsOf} {Material} {TextoBreveEs}"
		showNavButton="true"
		navButtonPress=".onNavBack"
		class="sapUiResponsiveContentPadding">
		<content>
			<Panel
				id="materialPanel"
				width="auto"
				class="sapUiNoContentPadding">
				<headerToolbar>
					<Toolbar>
						<Title text="{i18n>MaterialIDColon} {Material}" level="H2"/>
						<ToolbarSpacer />
						<Link text="{i18n>FlipToResume}" tooltip="{i18n>FlipToResume.tooltip}" press=".onShowResume"/>
					</Toolbar>
				</headerToolbar>
				<content>
					<f:SimpleForm
						editable="false"
						layout="ResponsiveGridLayout"
						labelSpanL="3" labelSpanM="3" emptySpanL="4" emptySpanM="4"
						columnsL="1" columnsM="1">
						<f:content>
							<Label text="{i18n>formID}"/>
							<Text text="{Material}"/>
							<Text text="{TextoBreveEs}"/>
						</f:content>
					</f:SimpleForm>
				</content>
			</Panel>
		</content>
	</Page>
</mvc:View>
